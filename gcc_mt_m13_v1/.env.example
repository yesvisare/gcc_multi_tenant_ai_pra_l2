# Redis Configuration (Caching Infrastructure)
REDIS_ENABLED=false
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here
REDIS_DB=0
REDIS_SSL=false

# Offline Mode (for development/testing without Redis)
OFFLINE=true

# PostgreSQL Configuration (Optional - for tenant metadata)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=tenant_metadata
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password_here

# Logging
LOG_LEVEL=INFO
