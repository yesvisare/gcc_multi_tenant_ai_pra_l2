# Database Configuration
# PostgreSQL connection string
DATABASE_URL=postgresql://localhost:5432/tenant_registry

# Redis connection string
REDIS_URL=redis://localhost:6379/0

# Logging Configuration
LOG_LEVEL=INFO

# Cache TTL Configuration (in seconds)
CACHE_TTL_CRITICAL=60      # 1 minute for critical fields (limits, status)
CACHE_TTL_STANDARD=300     # 5 minutes for standard fields (metadata)
CACHE_TTL_STATIC=3600      # 1 hour for static fields (tier configs)

# Note: This module uses local databases only (PostgreSQL + Redis)
# No external API keys are required (SERVICE = OFFLINE)
