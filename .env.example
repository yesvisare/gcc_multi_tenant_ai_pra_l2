# Prometheus Configuration
# Set to 'true' to enable Prometheus metrics server
PROMETHEUS_ENABLED=false

# Port for Prometheus metrics endpoint (default: 8000)
# Metrics will be available at http://localhost:8000/metrics
PROMETHEUS_PORT=8000

# Offline Mode
# Set to 'true' for notebook/local development without metrics server
OFFLINE=false

# Logging
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
