# Multi-Tenant RAG Service Configuration
# Services detected from script: PINECONE (vector DB) + OPENAI (embeddings/LLM)

# Pinecone Vector Database
PINECONE_ENABLED=false
PINECONE_API_KEY=your_pinecone_api_key_here

# OpenAI Embeddings and LLM
OPENAI_ENABLED=false
OPENAI_API_KEY=your_openai_api_key_here

# Redis Caching (optional)
REDIS_ENABLED=false
REDIS_HOST=localhost
REDIS_PORT=6379

# Offline Mode (for local development/testing without external services)
OFFLINE=false

# JWT Authentication
JWT_SECRET=your-secret-key-change-in-production

# Logging
LOG_LEVEL=INFO

# Database (for production deployment)
# DATABASE_URL=postgresql://user:password@localhost:5432/multi_tenant_rag
