# AWS S3 Configuration - SERVICE: AWS_S3 (detected from script Section 4)
# Set AWS_S3_ENABLED=true to enable S3 operations
AWS_S3_ENABLED=false
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1
AWS_DEFAULT_BUCKET=rag-docs-shared

# Alternative: Run in offline mode (for testing without AWS credentials)
OFFLINE=true

# Redis Configuration (for presigned URL caching)
REDIS_ENABLED=false
REDIS_HOST=localhost
REDIS_PORT=6379

# Database Configuration (for audit logging)
DATABASE_URL=postgresql://user:password@localhost:5432/storage_audit

# Logging
LOG_LEVEL=INFO

# Multi-Region Configuration (for data residency)
# Uncomment and configure additional regions as needed
# AWS_REGION_EU=eu-west-1
# AWS_REGION_INDIA=ap-south-1
# AWS_BUCKET_EU=rag-docs-eu
# AWS_BUCKET_INDIA=rag-docs-india
